{
  "description": "Pipeline for parsing nomad stdout logs",
  "processors": [{
      "grok": {
        "field": "message",
        "patterns":[
          "%{TIMESTAMP_ISO8601:nomad.log.timestamp} \\[%{WORD:nomad.log.severity}\\] %{GREEDYDATA:nomad.log.message}"
        ],
        "ignore_missing": true
      }
    },
    {
      "remove": {
        "field": "message"
      }
    },
    {
      "rename": {
        "field": "@timestamp",
        "target_field": "read_timestamp"
      }
    },
    {
      "date": {
        "field": "nomad.log.timestamp",
        "target_field": "@timestamp",
        "formats": ["YYYY/MM/DD'T'HH:mm:ss.SSSZZ"]
      }
    },
    {
      "remove": {
        "field": "nomad.log.timestamp"
      }
    }],
    "on_failure" : [{
      "set" : {
        "field" : "error.message",
        "value" : "{{ _ingest.on_failure_message }}"
      }
    }]
}
