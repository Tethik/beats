{
  "description": "Pipeline for parsing nomad error logs",
  "processors": [{
      "grok": {
        "field": "message",
        "patterns":[
          "\\s*%{DATA:nomad.log.timestamp} \\[%{WORD:nomad.log.severity}\\] %{GREEDYDATA:nomad.log.message}"
        ],
        "ignore_missing": true
      }
    },
    {
      "remove": {
        "field": "message"
      }
    },
    {
      "date": {
        "field": "nomad.log.timestamp",
        "target_field": "@timestamp",
        "formats": ["yyyy/MM/dd hh:mm:ss.SSSSSS"]
      }
    },
    {
      "remove": {
        "field": "nomad.log.timestamp"
      }
    }],
    "on_failure" : [{
      "set" : {
        "field" : "error.message",
        "value" : "{{ _ingest.on_failure_message }}"
      }
    }]
}